extends ../../_component/layout/template
block cool-config
  -
    var myTitle = 'Learn to code HTML Headings for beginners';
    var myDescription = 'Headings are defined with the <h1> to <h6> tags.';
    var landingPage = 'HTML';
    var activeLink = '/html/headings';

block awesome-content
  h1=myTitle
  p=myDescription
  h2 Examples of Headings
  h1 Heading 1
  h2 Heading 2
  h3 Heading h3
  h4 Heading 4
  h5 Heading 5
  h6 Heading 6