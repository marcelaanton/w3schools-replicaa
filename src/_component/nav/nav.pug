-
  var sideNavigationList = {
    'Home': {
      title: 'HTML and CSS',
      linkList: [
        { label: 'Learn HTML', link: `${baseUrl}/html`},
        { label: 'Learn CSS', link: `${baseUrl}/css`},
        { label: 'Learn JavaScript', link: `${baseUrl}/javascript`},
      ]
    },
    'HTML': {
      title: 'HTML Tutorial',
      linkList: [
        { label: 'HTML Elements', link: `${baseUrl}/html/elements`},
        { label: 'HTML Attributes', link: `${baseUrl}/html/attributes`},
        { label: 'HTML Headings', link: `${baseUrl}/html/headings`},
      ]
    },
    'CSS': {
      title: 'CSS Tutorial',
      linkList: [
        { label: 'CSS Colors', link: `${baseUrl}/css/colors`},
        { label: 'CSS Backgrounds', link: `${baseUrl}/css/backgrounds`},
        { label: 'CSS Borders', link: `${baseUrl}/css/borders`},
      ]
    },
    'JavaScript': {
      title: 'JavaScript Tutorial',
      linkList: [
        { label: 'JavaScript Comments', link: `${baseUrl}/javascript/comments`},
        { label: 'JavaScript Variables', link: `${baseUrl}/javascript/variables`},
        { label: 'JavaScript Operators', link: `${baseUrl}/javascript/operators`},
      ]
    },
  };
nav
  h3=sideNavigationList[landingPage].title
  each navigation in sideNavigationList[landingPage].linkList
    if navigation.link == activeLink
       a.current(href=`${baseUrl}${navigation.link}`)=navigation.label
    else
      a(href=`${baseUrl}${navigation.link}`)=navigation.label
  //- a(href="/html") Learn HTML
  //- a(href="/css") Learn CSS
  //- a(href="/javascript") Learn JavaScript